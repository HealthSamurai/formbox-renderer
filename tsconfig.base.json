{
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.base.tsbuildinfo",
    "target": "ES2023",
    "useDefineForClassFields": false,
    "experimentalDecorators": true,
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitReturns": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@formbox/fhir": ["packages/fhir/lib"],
      "@formbox/fhir/*": ["packages/fhir/lib/*"],
      "@formbox/theme": ["packages/theme/lib"],
      "@formbox/antd-theme": ["themes/antd-theme/lib"],
      "@formbox/mantine-theme": ["themes/mantine-theme/lib"],
      "@formbox/hs-theme": ["themes/hs-theme/lib"],
      "@formbox/nshuk-theme": ["themes/nshuk-theme/lib"],
      "@formbox/renderer": ["packages/renderer/lib"],
      "@formbox/renderer/*": ["packages/renderer/lib/*"]
    },
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
    "exactOptionalPropertyTypes": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitOverride": true
  }
}
