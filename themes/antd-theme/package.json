{
  "name": "@formbox/antd-theme",
  "version": "0.1.0",
  "description": "Ant Design theme for Formbox Renderer",
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HealthSamurai/formbox-renderer"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "style": "./dist/index.css"
    },
    "./style.css": {
      "import": "./dist/index.css"
    }
  },
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b ./tsconfig.lib.json && vite build",
    "dev": "echo \"@formbox/antd-theme: no dev server defined\"",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "test": "echo \"@formbox/antd-theme: no tests defined\""
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@linaria/core": "^6.3.0",
    "@linaria/react": "^6.3.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@wyw-in-js/vite": "^1.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@formbox/theme": "workspace:*",
    "antd": "^6.2.0"
  }
}
