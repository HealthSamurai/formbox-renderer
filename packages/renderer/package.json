{
  "name": "@formbox/renderer",
  "version": "0.0.6",
  "description": "Formbox Renderer for FHIR R4 and R5 Questionnaires",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HealthSamurai/formbox-renderer"
  },
  "homepage": "https://healthsamurai.github.io/formbox-renderer/",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b ./tsconfig.lib.json && vite build",
    "lint": "eslint .",
    "gen:fhir-types": "node ../../scripts/gen-fhir-common-types.mjs",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "lint-staged": "lint-staged",
    "format": "prettier --write .",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@formbox/theme": "workspace:*",
    "@lhncbc/ucum-lhc": "7.1.3",
    "@types/fhir": "^0.0.41",
    "@types/fuzzy-search": "^2.1.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@lhncbc/ucum-lhc": "^7.1.3",
    "classnames": "^2.5.1",
    "fhirpath": "^4.6.0",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "mobx-utils": "^6.1.1",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "fuzzy-search": "^3.2.1",
    "hashery": "^1.4.0"
  }
}
