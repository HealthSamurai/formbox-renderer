{
  "name": "@formbox/fhir",
  "version": "0.1.0",
  "description": "FHIR R4/R5 adapters and shared types for Formbox",
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HealthSamurai/formbox-renderer"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "echo \"@formbox/fhir: no dev server defined\"",
    "lint": "eslint .",
    "generate-types": "node --experimental-strip-types ./scripts/generate-types.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest --run"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@types/fhir": "^0.0.41"
  }
}
